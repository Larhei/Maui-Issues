    <Color x:Key="Primary300">#BFA2EB</Color>
    <Color x:Key="Primary400">#5E2FA7</Color>
    <Color x:Key="Primary">#420A98</Color>
    <Color x:Key="PrimaryDark">#420A98</Color>
    <Color x:Key="Primary600">#3C0990</Color>
    <Color x:Key="Primary700">#330785</Color>
    <Color x:Key="Primary800">#2B057B</Color>
    <Color x:Key="Primary900">#1D036A</Color>
    <Color x:Key="Tag1">#F9E58C</Color>
    <Color x:Key="Tag2">#CEE6F9</Color>
    <Color x:Key="Error">#AF3B17</Color>
    <Color x:Key="MidnightBlue">#AF3B17</Color>
    <Color x:Key="Secondary">#FAB141</Color>
    <Color x:Key="Tertiary">#2B0B98</Color>
    <Color x:Key="White">White</Color>
    <Color x:Key="Black">Black</Color>
    <Color x:Key="OffBlack">Black</Color>
