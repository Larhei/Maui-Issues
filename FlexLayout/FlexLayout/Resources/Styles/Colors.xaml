    <Color x:Key="Gray600">#404040</Color>
    <Color x:Key="Gray900">#212121</Color>
    <Color x:Key="Gray950">#141414</Color>
    <Color x:Key="Magenta">#AF3B17</Color>

    <SolidColorBrush x:Key="Neutral0Brush" Color="{StaticResource Neutral0}"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}"/>
    <SolidColorBrush x:Key="SurfaceBrush" Color="{StaticResource Surface}"/>
    <SolidColorBrush x:Key="DisabledBrush" Color="{StaticResource Primary150}"/>

    <SolidColorBrush x:Key="Primary050Brush" Color="{StaticResource Primary050}"/>
    <SolidColorBrush x:Key="Primary100Brush" Color="{StaticResource Primary100}"/>
    <SolidColorBrush x:Key="Primary150Brush" Color="{StaticResource Primary150}"/>
